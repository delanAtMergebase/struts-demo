<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.apache.struts2.showcase.model.CreditReport" table="credit_reports">
        <id
                name="reportId"
                type="integer"
                column="report_id"
                length="255"
        >
        </id>
        <property
                name="givenName"
                type="string"
                column="given_name"
                length="255"
        />
        <property
                name="surname"
                type="string"
                column="surname"
                length="255"
        />
        <property
                name="address"
                type="string"
                column="address"
                length="255"
        />
        <property
                name="birthYear"
                type="string"
                column="birth_year"
                length="255"
        />
        <property
                name="birthMonth"
                type="string"
                column="birth_month"
                length="255"
        />
        <property
                name="birthDay"
                type="string"
                column="birth_day"
                length="255"
        />
        <property
                name="socialInsuranceNumber"
                type="string"
                column="social_insurance_number"
                length="255"
        />
        <property
                name="creditRating"
                type="string"
                column="credit_rating"
                length="255"
        />
        <property
                name="salary"
                type="string"
                column="salary"
                length="256"
        />
    </class>
</hibernate-mapping>